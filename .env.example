# DuckLake Configuration
# Copy this file to .env and fill in actual values

DUCKLAKE_DUCKDB_DATABASE=:memory:
DUCKLAKE_CATALOG_DSN=dbname=ducklake_catalog host=localhost user=your_user password=your_password port=5433
DUCKLAKE_DATA_PATH=s3://ducklake/data/
DUCKLAKE_S3_REGION=us-east-1
DUCKLAKE_S3_ENDPOINT=localhost:9000
DUCKLAKE_S3_ACCESS_KEY_ID=your_access_key
DUCKLAKE_S3_SECRET_ACCESS_KEY=your_secret_key
DUCKLAKE_S3_URL_STYLE=path
DUCKLAKE_S3_USE_SSL=false
DUCKLAKE_SCHEMA=my_ducklake
